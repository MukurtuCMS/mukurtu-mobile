{
  "expo": {
    "name": "mukurtu-mobile",
    "description": "A very interesting project.",
    "slug": "mukurtu-mobile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.kanopi.mukurtu-mobile",
      "buildNumber": "1.0.2",
      "supportsTablet": true,
      "infoPlist": {
        "UIUserInterfaceStyle": "Light",
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.kanopi.mukurtu_mobile",
      "versionCode": 1
    }
  }
}
